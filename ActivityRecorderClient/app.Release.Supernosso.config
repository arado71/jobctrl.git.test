<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="WebsiteUrl" value="http://jobctrl.supernosso.intra/" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
    <add key="AppClassifier" value="Supernosso" xdt:Transform="Insert"/>
    <add key="IsRoamingStorageScopeNeeded" value="true" xdt:Transform="Insert"/>
  </appSettings>
  <log4net>
  <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender" xdt:Locator="Match(name)">
    <file value="${LOCALAPPDATA}\JobCTRL\Logs\JobCTRL.log" xdt:Transform="SetAttributes(value)" />
    <!-- Settings if we want to support Windows XP and later. -->
    <!-- <file value="${APPDATA}\JobCTRL\Logs\JobCTRL.log" xdt:Transform="SetAttributes(value)" />
    <maxSizeRollBackups value="1" xdt:Transform="SetAttributes(value)" />
    <maximumFileSize value="1MB" xdt:Transform="SetAttributes(value)" /> -->
  </appender>
    <appender name="CustomRollingDebugLogFileAppender" type="Tct.ActivityRecorderClient.Logging.CustomRollingDebugLogFileAppender" xdt:Locator="Match(name)">
    <file value="${LOCALAPPDATA}\JobCTRL\Logs\JobCTRL-Debug.log" xdt:Transform="SetAttributes(value)" />
    <!-- Settings if we want to support Windows XP and later. -->
    <!-- <file value="${APPDATA}\JobCTRL\Logs\JobCTRL-Debug.log" xdt:Transform="SetAttributes(value)" />
    <maxSizeRollBackups value="1" xdt:Transform="SetAttributes(value)" />
    <maximumFileSize value="1MB" xdt:Transform="SetAttributes(value)" /> -->
  </appender>
  </log4net>
  <system.serviceModel>
    <bindings>
      <customBinding xdt:Transform="Remove"/>
      <netTcpBinding xdt:Transform="Insert">
        <binding name="NetTcpBinding_IActivityRecorder" closeTimeout="00:01:00"
          openTimeout="00:01:00" receiveTimeout="00:10:00" sendTimeout="00:01:00"
          transactionFlow="false" transferMode="Buffered" transactionProtocol="OleTransactions"
          hostNameComparisonMode="StrongWildcard" listenBacklog="10" maxBufferPoolSize="12000001"
          maxBufferSize="5000001" maxConnections="10" maxReceivedMessageSize="5000001">
          <readerQuotas maxDepth="32" maxStringContentLength="8192" maxArrayLength="16384"
            maxBytesPerRead="4096" maxNameTableCharCount="16384" />
          <reliableSession ordered="true" inactivityTimeout="00:10:00"
            enabled="false" />
          <security mode="Message">
            <message clientCredentialType="UserName" />
          </security>
        </binding>
        <binding name="NetTcpBinding_IActiveDirectoryLoginService">
          <security mode="Message">
            <message clientCredentialType="Windows" />
          </security>
        </binding>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <endpointBehaviors>
        <behavior name="CustomBinding_Behav" xdt:Locator="Match(name)" xdt:Transform="Remove"/>
      </endpointBehaviors>
    </behaviors>
    <extensions xdt:Transform="Remove"/>
    <client>
      <endpoint address="net.tcp://jobctrl.supernosso.intra:9000/ActivityRecorderService"
        behaviorConfiguration="NetTcpBinding_Behav" binding="netTcpBinding"
        bindingConfiguration="NetTcpBinding_IActivityRecorder" contract="ActivityRecorderServiceReference.IActivityRecorder"
        name="NetTcpBinding_IActivityRecorder"
        xdt:Locator="Match(name)" xdt:Transform="Replace">
        <identity>
          <certificate encodedValue="" />
        </identity>
      </endpoint>
      <endpoint name="HttpBinding_IActivityRecorder" xdt:Locator="Match(name)" xdt:Transform="Remove"/>
      <endpoint address="net.tcp://jobctrl.supernosso.intra:9000/ActiveDirectoryLoginService"
        binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IActiveDirectoryLoginService"
        contract="ActivityRecorderServiceReference.IActiveDirectoryLoginService"
        name="NetTcpBinding_IActiveDirectoryLoginService"
        xdt:Transform="Insert" >
        <identity>
          <userPrincipalName value="jobctrlapp@supernosso.intra" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>