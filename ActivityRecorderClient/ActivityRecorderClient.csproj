    <Delete Files="$(ProjectDir)\JavaCapture\JC.JavaCapture.exe.config" TreatErrorsAsWarnings="true" />
    <Delete Files="$(ProjectDir)\JavaCapture\JC.JavaCapture.exe" TreatErrorsAsWarnings="true" />
    <Delete Files="$(ProjectDir)\JavaCapture\log4net.dll" TreatErrorsAsWarnings="true" />
  </Target>
  <Target Name="CheckMsiResources" Condition="'$(Configuration)' == 'Release'">
    <CheckResources wxs="$(ProjectDir)..\Deployment\MsiPackage\Product.wxs" binDir="$(ProjectDir)bin\$(Configuration)\" clientName="$(AssemblyName)" />
  </Target>
  <!--Tasks-->
  <UsingTask TaskName="TransformXml" AssemblyFile="..\Utilities\Microsoft.Web.Publishing.Tasks.dll" />
  <!--Inline Tasks-->
  <UsingTask TaskName="CheckResources" TaskFactory="CodeTaskFactory" AssemblyFile="$(MSBuildToolsPath)\Microsoft.Build.Tasks.Core.dll">
    <ParameterGroup>
      <Wxs ParameterType="System.String" Required="true" />
      <BinDir ParameterType="System.String" Required="true" />
      <ClientName ParameterType="System.String" Required="true" />
    </ParameterGroup>
    <Task>
      <Using Namespace="System.Collections.Generic" />
      <Using Namespace="System.IO" />
      <Using Namespace="System.Text.RegularExpressions" />
      <Using Namespace="Microsoft.Build.Framework" />
      <Code Type="Fragment" Language="cs"><![CDATA[
			Log.LogMessage("Checking resources inside Wxs: "+Wxs, MessageImportance.High);
			var lines = File.ReadLines(Wxs);
			var installedRes = new List<String>();
			foreach(var line in lines)
			{
				var result = Regex.Match(line, @"<File.*Source=""\$\(var\.ActivityRecorderClient\.TargetDir\)\\(.+)\\\$\(var\.ActivityRecorderClient\.TargetName\)\.resources\.dll""");
				if (result.Success)
				{
					installedRes.Add(result.Groups[1].Value);
					Log.LogMessage("Found resource: " + result.Groups[1].Value, MessageImportance.High);
				}
			}
			
			Log.LogMessage("Checking resources inside directory: "+BinDir, MessageImportance.High);
			var generatedRes = new List<String>();
			foreach(var dir in Directory.GetDirectories(BinDir))
			{
				foreach(var file in Directory.GetFiles(dir))
				{
					if(file.Contains(ClientName + ".resources.dll"))
					{
						Log.LogMessage("Found resource: " + file, MessageImportance.High);
						generatedRes.Add(Path.GetFileName(dir));
					}
				}
			}
			
			foreach(var res in generatedRes)
			{
				if(!installedRes.Contains(res)) {
					Log.LogMessage("Found mismatch: " + res, MessageImportance.High);
					Log.LogError(null, null, null, null, 0, 0, 0, 0, String.Format("Localization {0} is missing from {1}!", res, Wxs), new object[0]); 
				}
			}
		]]></Code>
    </Task>
  </UsingTask>
  <UsingTask TaskName="RegexTransform" TaskFactory="CodeTaskFactory" AssemblyFile="$(MSBuildToolsPath)\Microsoft.Build.Tasks.Core.dll">
    <ParameterGroup>
      <Items ParameterType="Microsoft.Build.Framework.ITaskItem[]" />
    </ParameterGroup>
    <Task>
      <Using Namespace="System.IO" />
      <Using Namespace="System.Text.RegularExpressions" />
      <Using Namespace="Microsoft.Build.Framework" />
      <Code Type="Fragment" Language="cs"><![CDATA[
          foreach(ITaskItem item in Items) {
            string fileName = item.GetMetadata("FullPath");
            string find = item.GetMetadata("Find");
            string replaceWith = item.GetMetadata("ReplaceWith");

            if(!File.Exists(fileName)) {
              Log.LogError(null, null, null, null, 0, 0, 0, 0, String.Format("Could not find file: {0}", fileName), new object[0]);
            }
            string content = File.ReadAllText(fileName);
            File.WriteAllText(
              fileName,
              Regex.Replace(
                content,
                find,
                replaceWith,
                RegexOptions.Multiline
              ),
              System.Text.Encoding.UTF8
            );
            Log.LogMessage("Updated file " + fileName, MessageImportance.High);
          }
        ]]></Code>
    </Task>
  </UsingTask>
