  <Target Name="TransfromConfigExec" Condition="'$(Configuration)' == 'Release' AND $(PublishProfile)!=''">
    <Error Condition="!Exists('Configuration\AppConfig$(PublishProfile).cs')" Text="AppConfig$(PublishProfile).cs file is missing" />
    <Copy SourceFiles="app.config" DestinationFiles="app.config.bak" Condition="!Exists('app.config.bak')" />
    <Copy SourceFiles="Configuration\AppConfig$(PublishProfile).cs" DestinationFiles="Configuration\AppConfig$(PublishProfile).cs.bak" Condition="!Exists('Configuration\AppConfig$(PublishProfile).cs.bak')" />
    <Copy SourceFiles="app.$(Configuration).config" DestinationFiles="app.config" />
    <OnError ExecuteTargets="ResetTransfromConfig" />
  </Target>
