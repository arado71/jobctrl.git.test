  <Target Name="SetVersionInfo4" Condition="'$(Configuration)' == 'Release'">
    <!--Update AssemblyInfo with WiX version for .Net 4.0 -->
    <RegexTransform Items="@(SetRegexTransform4)" />
  </Target>
  <Target Name="ResetVersionInfo" Condition="'$(Configuration)' == 'Release'">
    <!--Reset version in AssemblyInfo, don't leave WiX revision in code-->
    <RegexTransform Items="@(ResetRegexTransform)" />
  </Target>
  <!--Generate .msi package for .Net 4.0 -->
  <Target Name="BuildWix4" Condition="'$(Configuration)' == 'Release'">
    <MSBuild Projects="$(ProjectDir)..\Deployment\MsiPackage\MsiPackage.wixproj" Targets="Clean;Build" Properties="ClientAssemblyName=$(AssemblyName);UpgradeCode=$(UpgradeCode);MsiInstallScope=$(MsiInstallScope);IncludeService=$(IncludeService);Configuration=Release;Platform=MixedWix;TargetFrameworkVersion=v4.5;ProjectSync=$(ProjectSync);OcrPlugin=$(OcrPlugin);TesseractDir=$(TesseractDir)" />
    <Copy SourceFiles="$(ProjectDir)..\Deployment\MsiPackage\bin\$(Configuration)\JobCTRL4.msi" DestinationFiles="$(PublishDir)\$(PackageName).msi" />
  </Target>
  <!--Generate .zip package -->
  <Target Name="BuildZip" Condition="'$(Configuration)' == 'Release' AND '$(ZipPackage)' != ''">
    <Delete Files="$(PublishDir)\$(PackageName).zip" />
    <Copy SourceFiles="$(OutputPath)\log4net.dll;$(OutputPath)\log4net.dll" DestinationFiles="$(OutputPath)\ChromeInterop\log4net.dll;$(OutputPath)\FirefoxInterop\log4net.dll" />
    <Exec WorkingDirectory="$(OutputPath)" Command="&quot;$(ProjectDir)\..\Utilities\7za.exe&quot; a &quot;$(PublishDir)\$(PackageName).zip&quot; -r -x!Newtonsoft.Json.xml -x!*.pdb -x!logs -x!*.vshost* -x!JobCTRL*.application -x!app.publish -x!resources -x!jobctrl*.manifest" />
    <Exec WorkingDirectory="$(OutputPath)" Command="&quot;$(ProjectDir)\..\Utilities\7za.exe&quot; rn &quot;$(PublishDir)\$(PackageName).zip&quot; ChromeInterop v$(ApplicationBaseVersion)\ChromeInterop EdgeInterop v$(ApplicationBaseVersion)\EdgeInterop FirefoxInterop v$(ApplicationBaseVersion)\FirefoxInterop OutlookAddin v$(ApplicationBaseVersion)\OutlookAddin" />
  </Target>
  <!--Publish.htm Generation-->
  <ItemGroup>
    <PublishHtmlReplace Include="[PublisherName]">
      <ReplaceWith>$(PublisherName)</ReplaceWith>
      <InProject>false</InProject>
    </PublishHtmlReplace>
