<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="WebsiteUrl" value="http://pilot.jobctrl.intra" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
    <add key="AppClassifier" value="Pilot" xdt:Transform="Insert"/>
  </appSettings>
  <system.serviceModel>
    <bindings>
      <customBinding xdt:Transform="Remove"/>
      <netTcpBinding xdt:Transform="Insert">
        <binding name="NetTcpBinding_IActivityRecorder" closeTimeout="00:01:00"
          openTimeout="00:01:00" receiveTimeout="00:10:00" sendTimeout="00:01:00"
          transactionFlow="false" transferMode="Buffered" transactionProtocol="OleTransactions"
          hostNameComparisonMode="StrongWildcard" listenBacklog="10" maxBufferPoolSize="12000001"
          maxBufferSize="5000001" maxConnections="10" maxReceivedMessageSize="5000001">
          <readerQuotas maxDepth="32" maxStringContentLength="8192" maxArrayLength="16384"
            maxBytesPerRead="4096" maxNameTableCharCount="16384" />
          <reliableSession ordered="true" inactivityTimeout="00:10:00"
            enabled="false" />
          <security mode="Message">
            <message clientCredentialType="UserName" />
          </security>
        </binding>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <endpointBehaviors>
        <behavior name="CustomBinding_Behav" xdt:Locator="Match(name)" xdt:Transform="Remove"/>
      </endpointBehaviors>
    </behaviors>
    <extensions xdt:Transform="Remove"/>
    <client>
      <endpoint address="net.tcp://pilot.jobctrl.intra:9000/ActivityRecorderService"
        behaviorConfiguration="NetTcpBinding_Behav" binding="netTcpBinding"
        bindingConfiguration="NetTcpBinding_IActivityRecorder" contract="ActivityRecorderServiceReference.IActivityRecorder"
        name="NetTcpBinding_IActivityRecorder"
        xdt:Locator="Match(name)" xdt:Transform="Replace">
        <identity>
          <certificate encodedValue="" />
        </identity>
      </endpoint>
      <endpoint name="HttpBinding_IActivityRecorder" xdt:Locator="Match(name)" xdt:Transform="Remove"/>
    </client>
  </system.serviceModel>
</configuration>