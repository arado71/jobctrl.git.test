<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:svg="clr-namespace:Avalonia.Svg.Skia;assembly=Svg.Controls.Skia.Avalonia"
             xmlns:local="clr-namespace:ActivityRecorderClientAV"
             x:Class="ActivityRecorderClientAV.TaskControlAV">

<StackPanel Orientation="Vertical">

    <TextBlock x:Name="TaskFolder"
                Text="Folder Name"
                FontSize="14"
                FontFamily="{StaticResource InterTightFont}"
                FontWeight="Regular"
                Margin="125,15,0,0" />
    <TextBlock x:Name="TaskName"
                Text="Task Name"
                FontSize="16"
                FontFamily="{StaticResource InterTightFont}"
                FontWeight="Bold"
                Margin="125,0,0,0" />

</StackPanel>

</UserControl>