<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
	    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="JiraSyncTool.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
        </sectionGroup>
    </configSections>
    <connectionStrings/>
	<log4net>
		<appender name="RollingDebugFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="Logs\JiraSyncTool-Debug.log"/>
			<appendToFile value="true"/>
			<rollingStyle value="Size"/>
			<maxSizeRollBackups value="10"/>
			<maximumFileSize value="5000KB"/>
			<staticLogFileName value="true"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%2thread] %-5level %24.24logger{1} - %message%newline"/>
			</layout>
		</appender>
		<appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
			<file value="Logs\JiraSyncTool.log"/>
			<appendToFile value="true"/>
			<rollingStyle value="Size"/>
			<maxSizeRollBackups value="3"/>
			<maximumFileSize value="5000KB"/>
			<staticLogFileName value="true"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%2thread] %-5level %24.24logger{1} - %message%newline"/>
			</layout>
			<filter type="log4net.Filter.LevelRangeFilter">
				<levelMin value="INFO"/>
				<levelMax value="FATAL"/>
			</filter>
		</appender>
		<root>
			<level value="DEBUG"/>
			<appender-ref ref="RollingDebugFileAppender"/>
			<appender-ref ref="RollingFileAppender"/>
		</root>
	</log4net>
    <applicationSettings>
        <JiraSyncTool.Properties.Settings>
            <setting name="WorklogComment" serializeAs="String">
                <value>From JobCTRL
</value>
            </setting>
            <setting name="JiraAuthServer" serializeAs="String">
                <value/>
            </setting>
            <setting name="JiraBaseUrl" serializeAs="String">
                <value/>
            </setting>
            <setting name="JiraAppKey" serializeAs="String">
                <value/>
            </setting>
            <setting name="JiraOAuthClientId" serializeAs="String">
                <value/>
            </setting>
            <setting name="JiraSharedSecret" serializeAs="String">
                <value/>
            </setting>
            <setting name="JiraWorklogSyncInterval" serializeAs="String">
                <value>180</value>
            </setting>
            <setting name="JcFullSyncInterval" serializeAs="String">
                <value>60</value>
            </setting>
            <setting name="TargetRootProjectId" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="CompanyAuthCode" serializeAs="String">
                <value>00000000-0000-0000-0000-000000000000</value>
            </setting>
            <setting name="TimerIntervalInMinutes" serializeAs="String">
                <value>2</value>
            </setting>
            <setting name="JiraSyncTool_jobCTRLAPI_API" serializeAs="String">
                <value>http://dev.jobctrl.net/api/api.asmx</value>
            </setting>
            <setting name="IsServerJira" serializeAs="String">
                <value>False</value>
            </setting>
        </JiraSyncTool.Properties.Settings>
    </applicationSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/></startup></configuration>
