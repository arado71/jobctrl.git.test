        <behavior name="ActivityRecorderServiceBehavior">
          <serviceThrottling maxConcurrentCalls="2001" maxConcurrentSessions="2001" maxConcurrentInstances="2147483647"/>
          <serviceCredentials>
            <serviceCertificate findValue="tct.ActivityRecorder" storeLocation="LocalMachine" storeName="My" x509FindType="FindBySubjectName"/>
