<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
  </appSettings>
  <system.serviceModel>
    <bindings>
	  <basicHttpBinding>
        <binding name="BasicHttpBindingWithWinAuth" xdt:Transform="Insert">
          <security mode="Transport">
            <transport clientCredentialType="Windows" />
          </security>
        </binding>
      </basicHttpBinding>
      <netTcpBinding xdt:Transform="Replace">
        <binding name="NetTcpBinding_IActivityRecorder" closeTimeout="00:01:00"
          openTimeout="00:01:00" receiveTimeout="00:10:00" sendTimeout="00:01:00"
          transactionFlow="false" transferMode="Buffered" transactionProtocol="OleTransactions"
          hostNameComparisonMode="StrongWildcard" listenBacklog="10" maxBufferPoolSize="12000001"
          maxBufferSize="5000001" maxConnections="10" maxReceivedMessageSize="5000001">
          <readerQuotas maxDepth="32" maxStringContentLength="8192" maxArrayLength="100001"
            maxBytesPerRead="4096" maxNameTableCharCount="16384" />
          <reliableSession ordered="true" inactivityTimeout="00:10:00"
            enabled="false" />
          <security mode="Message">
            <message clientCredentialType="UserName" />
          </security>
        </binding>
        <binding name="NetTcpBinding_IActiveDirectoryLoginService">
          <security mode="Message">
            <message clientCredentialType="Windows" />
          </security>
        </binding>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <endpointBehaviors>
      </endpointBehaviors>
    </behaviors>
    <client>
	   <endpoint address="net.tcp://dev.jobctrl.net:9000/VoiceRecorderService"
        binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IActivityRecorder"
        behaviorConfiguration="NetTcpBinding_Behav"
        contract="ActivityRecorderServiceReference.IVoiceRecorder"
        name="NetTcpBinding_IActivityRecorder"
        xdt:Locator="Match(name)" xdt:Transform="Insert">
        <identity>
          <certificate encodedValue="" />
        </identity>
      </endpoint>
      <endpoint name="HttpBinding_IActivityRecorder" xdt:Locator="Match(name)" xdt:Transform="Remove"/>
	  <endpoint address="net.tcp://dev.jobctrl.net:9020/ActiveDirectoryLoginService"
          binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IActiveDirectoryLoginService"
          contract="ActivityRecorderServiceReference.IActiveDirectoryLoginService"
          name="NetTcpBinding_IActiveDirectoryLoginService"
          xdt:Transform="Insert" >
        <identity>
          <userPrincipalName value="JCAPP1\ccenter" />
        </identity>
      </endpoint>
      <endpoint address="https://dev.jobctrl.net:9002/ActiveDirectoryLoginService" contract="ActivityRecorderServiceReference.IActiveDirectoryLoginService"
                  binding="basicHttpBinding" bindingConfiguration="BasicHttpBindingWithWinAuth" behaviorConfiguration="NetTcpBinding_Behav"
                  name="HttpBinding_IActiveDirectoryLoginService"
                  xdt:Transform="Insert" />
    </client>
  </system.serviceModel>
</configuration>