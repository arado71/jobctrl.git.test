<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.jc360.autostart</string>

    <key>RunAtLoad</key>
    <true/>

    <key>LimitLoadToSessionType</key>
    <array>
      <string>Aqua</string>
    </array>

    <key>ProgramArguments</key>
    <array>
      <string>/usr/bin/open</string>
      <string>-a</string>
      <string>/Applications/JC360.app</string>
    </array>

    <key>StandardOutPath</key>
    <string>/var/log/jc360-autostart.out.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/jc360-autostart.err.log</string>
  </dict>
</plist>